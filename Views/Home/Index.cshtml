@model IEnumerable<_39_HoangVanHoan_Tuan08.Models.Theloaitin>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Them the loai moi", "ThemMoi")
</p>
<table>
    <tr>
        <th>
            Ten The loai
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tentheloai)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDLoai }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDLoai }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDLoai })
